    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.31.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
